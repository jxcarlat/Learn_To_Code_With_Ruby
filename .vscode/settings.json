        }
      },
      {
        "scope": "renpy.meta.color.#930dd4",
        "settings": {
          "foreground": "#930dd4"
        }
      },
      {
        "scope": "renpy.meta.color.#60af5e",
        "settings": {
          "foreground": "#60af5e"
        }
      },
      {
        "scope": "renpy.meta.color.#00ab4e",
